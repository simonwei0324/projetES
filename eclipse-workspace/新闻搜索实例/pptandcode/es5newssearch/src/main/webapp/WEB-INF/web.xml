<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>RecherchePatient</servlet-name>
  	<display-name>RecherchePatient</display-name>
  	<description></description>
  	<servlet-class>com.csdnes.controller.RecherchePatient</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>MediteurServlet</servlet-name>
  	<display-name>MediteurServlet</display-name>
  	<description></description>
  	<servlet-class>com.csdnes.controller.MediteurServlet</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>RecherchePatient</servlet-name>
  	<url-pattern>/RecherchePatient</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>MediteurServlet</servlet-name>
  	<url-pattern>/MediteurServlet</url-pattern>
  </servlet-mapping>
</web-app>
